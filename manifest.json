{
  "manifest_version": 3,
  "name": "Fluid Wallet",
  "version": "0.0.1",
  "description": "Cross-chain Ethereum wallet",
  "permissions": ["storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inpage.ts"],
      "matches": ["<all_urls>"]
    }
  ]
}
